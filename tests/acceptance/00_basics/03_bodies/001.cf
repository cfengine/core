# Test that log_failed without log_kept does not segfault (Mantis #1107)

body common control
{
      inputs => { "../../default.cf.sub" };
      bundlesequence  => { default("$(this.promise_filename)") };
      version => "1.0";
}

bundle agent test
{
  files:
      "$(G.testdir)/foo.txt"
      create => "true",
      action => test_log;
}

body action test_log
{
      log_failed => "stdout";
      log_string => "";
}

bundle agent check
{
  methods:
      "" usebundle => dcs_pass($(this.promise_filename));
}
