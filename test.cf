 bundle agent main
  {
    commands:
    any::
      "/bin/echo this is an unconstrained commands promise";
      Hr09::
        "/bin/echo 9am hour verbose logging"
if => windows,
          action => immediate_report_verbose;
  }
  body action immediate_report_verbose
  {
    expireafter => "0";
    report_level => "verbose";
  }
