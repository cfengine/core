body common control
{
      inputs => { "../../default.cf.sub", "namespaced.cf.subx" };
      bundlesequence => { test,
                          default("$(this.promise_filename)") };
      version => "1.0";
}

bundle agent init
{
}

bundle agent test
{
  classes:
    default:!any::
      "ok" not => "any";
}

bundle agent check
{
  reports:
    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}
