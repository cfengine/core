#######################################################
#
# Test hostinnetgroup
#
#######################################################

body common control
{
      inputs => { "../../default.cf.sub" };
      bundlesequence  => { default("$(this.promise_filename)") };
      version => "1.0";
}

#######################################################

bundle agent init
{
  vars:
      "dummy" string => "dummy";
}

#######################################################

bundle agent test
{
  classes:
      "ng_host" expression => hostinnetgroup("netgroup2");
}

#######################################################

bundle agent check
{
  classes:
      "ok" expression => "any"; # check not crashed

  reports:
    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}

