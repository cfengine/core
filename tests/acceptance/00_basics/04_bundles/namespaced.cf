# Redmine#4025

body common control
{
      inputs => { "../../default.cf.sub", "namespaced.cf.sub" };
      bundlesequence => {
                          ns1:mytest("x"),
                          ns2:mytest("x", "y"),
                          default("$(this.promise_filename)") };
      version => "1.0";
}

bundle agent init
{
}

bundle agent test
{
}

bundle agent check
{
  vars:
      "checks" slist => { "ns1:ns1global", "ns2:ns2global",
                          "ns1global", "ns2global",
                          "ok_ns1string", "ok_ns2string"
      };

  classes:
      "ok_ns1string" expression => strcmp($(ns1:mytest.ns1string), $(this.promise_dirname));
      "ok_ns2string" expression => strcmp($(ns2:mytest.ns2string), $(sys.fqhost));

      "ok" and => { "ns1:ns1global", "ns2:ns2global",
                    "!ns1global", "!ns2global",
                    "ok_ns1string", "ok_ns2string",
      };

  reports:
    EXTRA::
      "class $(checks) is ON" ifvarclass => $(checks);
    EXTRA::
      "class $(checks) is OFF" ifvarclass => "!$(checks)";
    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}
