# test isjsonexists() isjsonfalse() isjsontrue() isjsonprimitive() isjsoncontainer() isjsonarray() isjsonobject() 

body common control
{
      inputs => { "../../default.cf.sub" };
      bundlesequence  => { default("$(this.promise_filename)") };
      version => "1.0";
    nova_edition::
      host_licenses_paid => "5";
}

bundle agent init
{
  vars:
      "dummy" string => "dummy";
}

bundle agent test
{
  vars:
      "dummy" string => "dummy";
}

bundle agent check
{
  classes:
      "ok1" expression => isjsonexists("{\"x\":1, \"y\":2}", "");
      "ok2" expression => isjsonfalse("{\"x\":false, \"y\":2}", "[\"x\"]");
      "ok3" expression => isjsontrue("{\"x\":false, \"y\":true}", "[\"y\"]");
      "ok4" expression => isjsonprimitive("{\"x\":[100], \"y\":true}", "[\"x\", 0]");
      "ok5" expression => isjsoncontainer("{\"x\":1, \"y\":2}", "");
      "ok6" expression => isjsonarray("{\"x\":[1], \"y\":2}", "[\"x\"]");
      "ok7" expression => isjsonobject("{\"x\":1, \"y\":2}", "");
      "ok" and => { "ok1", "ok2", "ok3", "ok4", "ok5", "ok6", "ok7" };

  reports:

    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}
