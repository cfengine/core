#######################################################
#
# Test that we don't crash when slist has very long elements
# Redmine:3957 (https://cfengine.com/dev/issues/3957)
#
#######################################################

body common control
{
      inputs => { "../../default.cf.sub" };
      bundlesequence  => { default("$(this.promise_filename)") };
      version => "1.0";
}

#######################################################

bundle agent init
{
}

#######################################################

bundle agent test
{
  vars:
    "keys" slist => {
"ssh-dss  root@host1",
"ssh-dss  root@host2",};

}

#######################################################

bundle agent check
{
  classes:
      "ok" expression => "any",
        comment => "If we made it to here, we didn't crash";

  reports:
    DEBUG::
      "$(this.bundle): Got key $(test.keys)";
    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}
