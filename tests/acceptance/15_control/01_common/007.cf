#######################################################
#
# Test ignore_missing_bundles, expect to pass, methods
#
#######################################################

body common control
{
      inputs => { "../../default.cf.sub" };
      bundlesequence  => { default("$(this.promise_filename)") };
      version => "1.0";
      ignore_missing_bundles => "on";
}

#######################################################
#
# bundle agent init
# {
# vars:
#         "dummy" string => "dummy";
# }
#
#######################################################

bundle agent test
{
  vars:
      "dummy" string => "dummy";
}

#######################################################

bundle agent check
{
  vars:
      "dummy" string => "dummy";

  classes:
      "ok" expression => "any";

  reports:
    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}

