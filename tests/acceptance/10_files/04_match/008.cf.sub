#######################################################
#
# Delete a file in a nonexistent directory.
#
#######################################################

body common control
{
  bundlesequence  => { "delete" };
  version => "1.0";
}

#######################################################

bundle agent delete
{
  files:
    "/abc/nofile"
      delete => test_delete;

  reports:
    "CHECKPOINT";
}

body delete test_delete
{
  dirlinks => "delete";
  rmdirs   => "true";
}
