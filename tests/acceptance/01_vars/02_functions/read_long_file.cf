# Test that long file is successfully read by readstringlist() function

body common control
{
      inputs => { "../../default.cf.sub" };
      bundlesequence => { default("$(this.promise_filename)") };
      version => "1.0";
}

bundle agent init
{
}

bundle agent test
{
  vars:
      "value" slist => readstringlist("$(this.promise_filename).txt", "", "\n", 100000, 1000000);
}

bundle agent check
{
  vars:
      "length" int => length("test.value");
      "last" string => nth("test.value", 599);
  classes:
      "ok1" expression => strcmp("$(length)", "600");
      "ok2" expression => strcmp("$(last)", "line600");
      "ok" and => { "ok1", "ok2" };

  reports:
    DEBUG.ok1::
      "passed1";
    DEBUG.ok2::
      "passed2";
    DEBUG.!ok1::
      "failed1 $(length) != 600";
    DEBUG.!ok2::
      "failed2 $(last) != line600";

    ok::
      "$(this.promise_filename) Pass";
    !ok::
      "$(this.promise_filename) FAIL";
}
