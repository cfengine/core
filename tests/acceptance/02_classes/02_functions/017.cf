#######################################################
#
# Test classmatch()
#
#######################################################

body common control
{
  inputs => { "../../default.cf.sub" };
  bundlesequence  => { default("$(this.promise_filename)") };
  version => "1.0";
nova_edition::
  host_licenses_paid => "5";
}

#######################################################

bundle agent init
{
vars:
        "dummy" string => "dummy";
}

#######################################################

bundle agent test
{
vars:
        "dummy" string => "dummy";
}

#######################################################

bundle agent check
{
classes:
        "ok" and => {
                    classmatch("any"),
                    classmatch("cfengine"),
                    classmatch("cf.*"),
                    classmatch("Hr.*"),
                    classmatch("Yr.*"),
                    classmatch("Min.*"),
                    };

reports:
    ok::
        "$(this.promise_filename) Pass";
    !ok::
        "$(this.promise_filename) FAIL";
}

