# Check that we are not trying to load "cf_null" as an input_file

body common control
{
      inputs => { @(g.inputs) };
      bundlesequence  => { check };
      version => "1.0";
}

bundle common g
{
  vars:
      "inputs" slist => { "cf_null" };
}

bundle agent check
{
  reports:
    DEBUG::
      "This test should pass as a precursor to a bunch of related failures";
    any::
      "$(this.promise_filename) Pass";
}
