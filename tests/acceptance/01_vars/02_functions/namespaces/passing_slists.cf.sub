body file control
{
  namespace => "b";
}

bundle agent test_in_namespace(passed_list)
{
  vars:
      "mylist" slist => { @(passed_list) };
      "joined" string => join(",", "mylist");
}

bundle agent check_in_namespace(test)
{
  methods:
      "any" usebundle => default:default_check_strcmp("$(b:test_in_namespace.joined)", "list", "$(test)", "no");
}
